<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.applicationjoseph.TelaFragmento">

    <Button
        android:id="@+id/btnFrag1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fragmento 1"
        android:layout_marginStart="48dp"
        android:layout_marginLeft="41dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp" />

    <Button
        android:id="@+id/btnFrag2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fragmento 2"
        android:layout_marginStart="58dp"
        tools:layout_editor_absoluteY="8dp"
        android:layout_marginRight="0dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/btnFrag1"
        android:layout_marginLeft="8dp"
        app:layout_constraintHorizontal_bias="0.233" />

    <FrameLayout
        android:id="@+id/Frame_Container"
        android:layout_width="373dp"
        android:layout_height="442dp"
        tools:layout_editor_absoluteX="6dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/btnFrag1">

    </FrameLayout>
</android.support.constraint.ConstraintLayout>

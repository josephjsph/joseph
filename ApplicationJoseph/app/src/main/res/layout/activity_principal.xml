<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_centerHorizontal="false"
    android:layout_centerInParent="false"
    android:layout_centerVertical="false"
    android:orientation="vertical"
    tools:context="com.example.applicationjoseph.ActivityPrincipal">

    <TextView
        android:id="@+id/textView"
        android:layout_width="235dp"
        android:layout_height="42dp"
        android:text="Página Principal"
        android:textAlignment="center"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.032" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="15dp"
        android:onClick="OnClcEdtText"
        android:text="Texto"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <Button
        android:id="@+id/btnTelaData"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="44dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="15dp"
        android:onClick="OnClcTelaData"
        android:text="Data"
        app:layout_constraintLeft_toRightOf="@+id/button"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <Button
        android:id="@+id/btnTelaTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="176dp"
        android:layout_marginStart="176dp"
        android:layout_marginTop="15dp"
        android:onClick="OnClcTelaEditText"
        android:text="TextView"
        android:textSize="12sp"
        app:layout_constraintLeft_toRightOf="@+id/button"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <Button
        android:id="@+id/btnTelaImage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:onClick="OnClcTelaImage"
        android:text="Image"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button"
        android:layout_marginStart="16dp" />

    <Button
        android:id="@+id/btnTelaCheckBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="44dp"
        android:onClick="OnClcTelaCheckBox"
        android:text="CheckBox"
        android:textSize="12sp"
        app:layout_constraintLeft_toRightOf="@+id/btnTelaImage"
        app:layout_constraintTop_toBottomOf="@+id/btnTelaData"
        android:layout_marginStart="44dp" />

    <Button
        android:id="@+id/btnRadioButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="176dp"
        android:onClick="OnClcTelaRadioButton"
        android:text="RadioButton"
        android:textSize="9sp"
        app:layout_constraintLeft_toRightOf="@+id/btnTelaImage"
        app:layout_constraintTop_toBottomOf="@+id/btnTelaTextView"
        android:layout_marginStart="176dp" />

    <Button
        android:id="@+id/btnTelaSpiner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:onClick="OnClcTelaSpiner"
        android:text="Spiner"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnTelaImage"
        android:layout_marginStart="16dp" />

    <Button
        android:id="@+id/btnTelaSeekBar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="44dp"
        android:onClick="OnClcTelaSeekBar"
        android:text="SeekBar"
        app:layout_constraintLeft_toRightOf="@+id/btnTelaSpiner"
        app:layout_constraintTop_toBottomOf="@+id/btnTelaCheckBox"
        android:layout_marginStart="44dp" />

    <Button
        android:id="@+id/btnTelaIntentsBrowser"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="44dp"
        android:onClick="OnClcTelaIntentsBrowser"
        android:text="Browser"
        app:layout_constraintLeft_toRightOf="@+id/btnTelaSeekBar"
        app:layout_constraintTop_toBottomOf="@+id/btnRadioButton"
        android:layout_marginStart="44dp" />

    <Button
        android:id="@+id/btnActivity2"
        android:layout_width="88dp"
        android:layout_height="48dp"
        android:layout_marginLeft="16dp"
        android:text="activity com Parâmetros"
        android:textSize="9sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnTelaSpiner"
        android:layout_marginStart="16dp" />

    <Button
        android:id="@+id/btnTelaMenu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="44dp"
        android:onClick="OnClcTelaMenu"
        android:text="Menu"
        app:layout_constraintLeft_toRightOf="@+id/btnActivity2"
        app:layout_constraintTop_toBottomOf="@+id/btnTelaSeekBar"
        android:layout_marginStart="44dp" />

    <Button
        android:id="@+id/btnTelaFragmento"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="44dp"
        android:onClick="OnClcTelaFragmento"
        android:text="Fragmento"
        android:textSize="10sp"
        app:layout_constraintLeft_toRightOf="@+id/btnTelaMenu"
        app:layout_constraintTop_toBottomOf="@+id/btnTelaIntentsBrowser"
        android:layout_marginStart="44dp" />

    <Button
        android:id="@+id/btnTelaMenuDeslisante"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:onClick="OnClcTelaMenuDeslisante"
        android:text="Menu Deslisante"
        android:textSize="7sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnActivity2"
        android:layout_marginStart="16dp" />

    <Button
        android:id="@+id/btnTelaLayouts"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="44dp"
        android:layout_marginTop="0dp"
        android:text="Layouts"
        android:onClick="OnClcTelaLayouts"
        app:layout_constraintLeft_toRightOf="@+id/btnTelaMenuDeslisante"
        app:layout_constraintTop_toBottomOf="@+id/btnTelaMenu" />

    <Button
        android:id="@+id/btnTelaListView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ListView"
        android:onClick="OnClcTelaListView"
        android:layout_marginTop="0dp"
        app:layout_constraintTop_toBottomOf="@+id/btnTelaFragmento"
        app:layout_constraintLeft_toRightOf="@+id/btnTelaLayouts"
        android:layout_marginLeft="44dp" />

    <Button
        android:id="@+id/btnProximaTela"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="45dp"
        android:layout_marginTop="16dp"
        android:text="Avançar"
        android:onClick="OnClcTelaPrincipal2"
        app:layout_constraintLeft_toRightOf="@+id/textView"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>
